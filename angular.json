{
  "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
  "version": 1,
  "newProjectRoot": "projects",
  "projects": {
    "reec": {
      "projectType": "library",
      "root": "projects/reec",
      "sourceRoot": "projects/reec/src",
      "prefix": "lib",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-ng-packagr:build",
          "options": {
            "tsConfig": "projects/reec/tsconfig.lib.json",
            "project": "projects/reec/ng-package.json"
          },
          "configurations": {
            "production": {
              "tsConfig": "projects/reec/tsconfig.lib.prod.json"
            }
          }
        },
        "test": {
          "builder": "@angular-devkit/build-angular:karma",
          "options": {
            "main": "projects/reec/src/test.ts",
            "tsConfig": "projects/reec/tsconfig.spec.json",
            "karmaConfig": "projects/reec/karma.conf.js"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "projects/reec/tsconfig.lib.json",
              "projects/reec/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    }
  },
  "cli": {
    "analytics": "1eaa3877-a569-4e3c-b269-0b20efef1758"
  },
  "defaultProject": "reec",
  "schematics": {
    "@schematics/angular:component": {
      "styleext": "scss"
    }
  }
}